(function () {
  const BATCH = 10;

  function setupList(container) {
    const rows = Array.from(container.querySelectorAll('.casino-row'));
    if (rows.length <= BATCH) return;

    rows.slice(BATCH).forEach(row => row.classList.add('clm-hidden'));

    const wrap = document.createElement('div');
    wrap.className = 'casino-load-more-wrapper';

    const btn = document.createElement('button');
    btn.type = 'button';
    btn.className = 'play-button-btn casino-load-more-btn';
    btn.setAttribute('aria-expanded', 'false');
    btn.innerHTML = `
      <div class="play-button w-100" style="padding:39px;">
        <span class="f-bold" style="color:white;margin-top:0;">Casino +</span>
      </div>
    `;
    wrap.appendChild(btn);
    container.insertAdjacentElement('afterend', wrap);

    btn.addEventListener('click', function () {
      const hidden = rows.filter(r => r.classList.contains('clm-hidden')).slice(0, BATCH);
      hidden.forEach(r => r.classList.remove('clm-hidden'));
      const anyLeft = rows.some(r => r.classList.contains('clm-hidden'));
      if (!anyLeft) btn.style.display = 'none';
    });
  }

  function init() {
    document.querySelectorAll('.casino-toplist-rows').forEach(setupList);
  }

  if (document.readyState === 'loading') {
    document.addEventListener('DOMContentLoaded', init);
  } else {
    init();
  }
})();
